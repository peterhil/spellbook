{
    "name": "Spellbook",
    "version": "0.5.0",
    "author": "Peter HillerstroÌˆm <peter.hillerstrom@gmail.com>",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "img/spellbook_icon.png",
        "default_popup": "views/popup.html"
    },
    "background": {
        "persistent": true,
        "page": "views/background.html"
    },
    "default_locale": "en",
    "description": "__MSG_meta_description__",
    "icons": {
        "64": "img/spellbook_icon_bookmarked.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+E",
                "mac": "MacCtrl+E"
            },
            "description": "__MSG_execute_browser_action__"
        }
    },
    "homepage_url": "https://github.com/peterhil/spellbook",
    "permissions": [
        "activeTab",
        "bookmarks",
        "tabs"
    ]
}
